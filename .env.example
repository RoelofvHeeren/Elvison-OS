VITE_API_BASE_URL=http://localhost:3001/

# Sheet MCP endpoint
SHEET_MCP_URL=https://sheet-mcp-v2-production.up.railway.app/mcp

# Google Sheet ID to read/write leads
SHEET_ID=your_sheet_id

GSHEETS_CLIENT_ID=your_google_client_id
GSHEETS_CLIENT_SECRET=your_google_client_secret
GSHEETS_REDIRECT_URI=http://localhost:3001/api/auth/google/callback

# OpenAI API key for calling workflow
OPENAI_API_KEY=your_openai_api_key

# Lead generation agent workflow ID and version
WORKFLOW_ID=wf_your_workflow_id
WORKFLOW_VERSION=20

# Optional: leave blank if your Sheet MCP does not require auth
MCP_API_KEY=

# JWT Secret for authentication (generate with: openssl rand -base64 32)
JWT_SECRET=your_secret_key_here_change_in_production

# Owner account (for migration)
OWNER_EMAIL=owner@elvison.ai
OWNER_NAME=System Owner

# PostgreSQL Database URL
DATABASE_URL=postgresql://user:password@localhost:5432/elvison
